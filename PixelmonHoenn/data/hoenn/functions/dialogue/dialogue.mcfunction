#-------------------------
#Dialogue 59 - Route 110
#Rival Post-Battle

execute as @s[scores={DialogueTrigger=59}] run clear @s minecraft:nether_brick

tellraw @s[scores={DialogueTrigger=59,TalkTime=3,Rival=1}] ["",{"text":"<May> "},{"selector":"@s"},{"text":", you've been busy training, too, haven't you?"}]
tellraw @s[scores={DialogueTrigger=59,TalkTime=10,Rival=1}] {"text":"<May> Oops! But look at us! Our teams look pretty tired."}
tellraw @s[scores={DialogueTrigger=59,TalkTime=18,Rival=1}] {"text":"<May> Let's get them taken care of first."}

execute as @s[scores={DialogueTrigger=59,TalkTime=25,Rival=1}] run pokeheal
execute as @s[scores={DialogueTrigger=59,TalkTime=25,Rival=1}] run playsound pixelmon:pixelmon.block.healeractivate ambient @s ~ ~ ~ 1 1 1
tellraw @s[scores={DialogueTrigger=59,TalkTime=25,Rival=1}] {"text":"Your team's health was restored!","italic":true,"color":"gray"}

tellraw @s[scores={DialogueTrigger=59,TalkTime=32,Rival=1}] {"text":"<May> And now I think you deserve a reward, too! Here, this is from me!"}

execute as @s[scores={DialogueTrigger=59,TalkTime=40,Rival=1}] run give @s pixelmon:item_finder
execute as @s[scores={DialogueTrigger=59,TalkTime=40,Rival=1}] run playsound keyitem ambient @s ~ ~ ~ 1 1 1
tellraw @s[scores={DialogueTrigger=59,TalkTime=40,Rival=1}] ["",{"selector":"@s","italic":true,"color":"gray"},{"text":" recieved the Dowsing Machine!","italic":true,"color":"gray"}]

tellraw @s[scores={DialogueTrigger=59,TalkTime=47,Rival=1}] {"text":"<May> That's a Dowsing Machine. Try it out—if there's a hidden item anywhere near you, that little thing will react to tell you."}
tellraw @s[scores={DialogueTrigger=59,TalkTime=57,Rival=1}] {"text":"<May> Pay attention, and it'll lead you right to items!"}
tellraw @s[scores={DialogueTrigger=59,TalkTime=64,Rival=1}] {"text":"<May> And it's fun to see all those waving bits above your head."}
tellraw @s[scores={DialogueTrigger=59,TalkTime=71,Rival=1}] {"text":"<May> Try it, and I bet you'll be hooked!"}
tellraw @s[scores={DialogueTrigger=59,TalkTime=78,Rival=1}] ["",{"text":"<May> See you later then, "},{"selector":"@s"},{"text":"! Let's meet again!"}]
tellraw @s[scores={DialogueTrigger=59,TalkTime=86,Rival=1}] {"text":"<May> I hope we'll both be able to meet even more Pokémon on our journeys!"}

#tps May out
execute as @s[scores={DialogueTrigger=59,TalkTime=94,Rival=1}] run particle cloud -1419 69 -133 1 1 1 0.15 300
execute as @s[scores={DialogueTrigger=59,TalkTime=94,Rival=1}] run tp @e[type=pixelmon:npc_trainer,x=-1419,y=68,z=-133,distance=..3] 10000000 -50000 -10000000



tellraw @s[scores={DialogueTrigger=59,TalkTime=3,Rival=2}] ["",{"text":"<Brendan> Sheesh, "},{"selector":"@s"},{"text":", when did you go and train your Pokémon up that well?"}]
tellraw @s[scores={DialogueTrigger=59,TalkTime=12,Rival=2}] {"text":"<Brendan> It's, like, I don't know... It's pretty surprising, OK!"}
tellraw @s[scores={DialogueTrigger=59,TalkTime=20,Rival=2}] {"text":"<Brendan> Both our teams look pretty wiped out after that, huh!"}
tellraw @s[scores={DialogueTrigger=59,TalkTime=27,Rival=2}] {"text":"<Brendan> Here, let me help them out."}

execute as @s[scores={DialogueTrigger=59,TalkTime=34,Rival=2}] run pokeheal
execute as @s[scores={DialogueTrigger=59,TalkTime=34,Rival=2}] run playsound pixelmon:pixelmon.block.healeractivate ambient @s ~ ~ ~ 1 1 1
tellraw @s[scores={DialogueTrigger=59,TalkTime=34,Rival=2}] {"text":"Your team's health was restored!","italic":true,"color":"gray"}

tellraw @s[scores={DialogueTrigger=59,TalkTime=41,Rival=2}] {"text":"<Brendan> And this is for you."}

execute as @s[scores={DialogueTrigger=59,TalkTime=48,Rival=2}] run give @s pixelmon:item_finder
