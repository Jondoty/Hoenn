tellraw @s[scores={DialogueTrigger=59,TalkTime=48,Rival=2}] ["",{"selector":"@s","italic":true,"color":"gray"},{"text":" recieved the Dowsing Machine!","italic":true,"color":"gray"}]

tellraw @s[scores={DialogueTrigger=59,TalkTime=55,Rival=2}] {"text":"<Brendan> That's a Dowsing Machine. Use it to root around for items that aren't visible."}
tellraw @s[scores={DialogueTrigger=59,TalkTime=63,Rival=2}] {"text":"<Brendan> If it senses something nearby, it'll react, see?"}
tellraw @s[scores={DialogueTrigger=59,TalkTime=70,Rival=2}] {"text":"<Brendan> Truth is, once I started using it, I got pretty hooked on it. It's addicting!"}
tellraw @s[scores={DialogueTrigger=59,TalkTime=78,Rival=2}] {"text":"<Brendan> But I recommend you give it a try."}
tellraw @s[scores={DialogueTrigger=59,TalkTime=85,Rival=2}] {"text":"<Brendan> I guess I'm off to get started surveying the next area then."}
tellraw @s[scores={DialogueTrigger=59,TalkTime=92,Rival=2}] ["",{"text":"<Brendan> I hope you meet a lot of Pokémon, too, "},{"selector":"@s"},{"text":"!"}]

